@model FoodDelivery.Entities.Concrete.Company


<div class="panel panel-primary">
    <div class="panel-heading">
        <h4>@Model.CompanyName</h4>
    </div>
    <div class="panel-body">
        <div class="form-group">
            <h4><b>E-Mail: </b>@Model.Email</h4>
        </div>
        <div class="form-group">
            <h4><b>Şifre:</b> @Model.Password</h4>
        </div>
        <div class="form-group">
            @Html.ActionLink("Çıkış Yap", "Logout", "Company", null, new { @class = "btn btn-danger form-control" })
        </div>
    </div>
</div>